<!DOCTYPE html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">

    <!-- Site Title -->
    <title>Ujjawal Sanskar</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/assets/img/Banner/Logo_Ujjwal.png" />

    <!-- CSS here -->
    <link rel="stylesheet" href="~/Content/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/assets/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/Content/assets/css/venobox.min.css">
    <link rel="stylesheet" href="~/Content/assets/css/animate.min.css">
    <link rel="stylesheet" href="~/Content/assets/css/keyframe-animation.css">
    <link rel="stylesheet" href="~/Content/assets/css/odometer.min.css">
    <link rel="stylesheet" href="~/Content/assets/css/nice-select.css">
    <link rel="stylesheet" href="~/Content/assets/css/daterangepicker.css">
    <link rel="stylesheet" href="~/Content/assets/css/swiper.min.css">
    <link rel="stylesheet" href="~/Content/assets/css/main.css">
    <link href="~/Content/assets/iziToast/iziToast.min.css" rel="stylesheet" />
    <script src="~/Content/assets/iziToast/iziToast.min.js"></script>


    <!-- SEO Part -->
    <meta charset="UTF-8">
    <meta name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=0'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="title" content="Ujjawal Sanskar" />
    <meta name="image" content="~/Areas/Admin/Content/assets/images/favicon-32x32.png" />
    <meta property="site_name" content="Ujjawal Sanskar" />

    <meta property="og:locale" content="en_US">
    <meta property="og:type" content="website">
    <meta property="og:title" content="Ujjawal Sanskar">
    <meta property="og:site_name" content="Ujjawal Sanskar">
    <meta property="og:image" content="~/Areas/Admin/Content/assets/images/favicon-32x32.png">
    <meta property="og:image:secure_url" content="~/Areas/Admin/Content/assets/images/favicon-32x32.png">
    <meta property="og:image:width" content="176">
    <meta property="og:image:height" content="52">
    <meta property="og:image:alt" content="thumnail">
    <meta property="og:image:type" content="image/png">
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="Ujjawal Sanskar">
</head>

<body>
    @Html.Action("Header", "Home")
    <!-- /.mobile-side-menu -->
    <div class="mobile-side-menu-overlay"></div>

    <div id="preloader">
        <div class="spinner-logo"><img src="~/Content/assets/img/favicon.png" alt="logo"></div>
        <div class="spinner"></div>
    </div>
    @RenderBody()
    @Html.Action("Footer", "Home")    <!-- ./ footer-section -->

    <div id="scrollup">
        <button id="scroll-top" class="scroll-to-top"><i class="fa-regular fa-arrow-up-long"></i></button>
    </div>
    <!--scrollup-->
    <!-- JS here -->
    <script src="~/Content/assets/js/vendor/jquary-3.6.0.min.js"></script>
    <script src="~/Content/assets/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js"></script>
    <script src="~/Content/assets/js/vendor/bootstrap-bundle.js"></script>
    <script src="~/Content/assets/js/vendor/jquery.isotope.js"></script>
    <script src="~/Content/assets/js/vendor/waypoints.min.js"></script>
    <script src="~/Content/assets/js/vendor/moment.min.js"></script>
    <script src="~/Content/assets/js/vendor/daterangepicker.min.js"></script>
    <script src="~/Content/assets/js/vendor/venobox.min.js"></script>
    <script src="~/Content/assets/js/vendor/odometer.min.js"></script>
    <script src="~/Content/assets/js/vendor/countdown.js"></script>
    <script src="~/Content/assets/js/vendor/meanmenu.js"></script>
    <script src="~/Content/assets/js/vendor/smooth-scroll.js"></script>
    <script src="~/Content/assets/js/vendor/imagesloaded-pkgd.js"></script>
    <script src="~/Content/assets/js/vendor/wow.min.js"></script>
    <script src="~/Content/assets/js/vendor/jquery.carouselTicker.js"></script>
    <script src="~/Content/assets/js/vendor/nice-select.js"></script>
    <script src="~/Content/assets/js/vendor/swiper.min.js"></script>
    <script src="~/Content/assets/js/main.js"></script>
</body>
</html>
